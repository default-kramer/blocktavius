<Window x:Class="Blocktavius.AppDQB2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Blocktavius.AppDQB2"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1100">
    <ContentControl Content="{Binding CurrentContent}">
        <ContentControl.Resources>
            <DataTemplate DataType="{x:Type local:StartupVM}">
                <local:StartupControl></local:StartupControl>
            </DataTemplate>
            <DataTemplate DataType="{x:Type local:ProjectVM}">
                <local:ProjectControl></local:ProjectControl>
            </DataTemplate>
        </ContentControl.Resources>
    </ContentControl>
</Window>
